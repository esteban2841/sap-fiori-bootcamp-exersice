<mvc:View controllerName="com.bootcamp.sapui5.testproject.controller.Home"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <VBox id="124" >
            <Button id="412" text="{i18n>consultar}" press="onPress"></Button>
            <Text text="Hi, my name is Esteban" id="5124" ></Text>
        </VBox>
        <Table items="{/Products}">
            <columns>
                <Column header="ID" />
                <Column header="Name"/>
                <Column header="Status"/>
            </columns>
            <items>
                <ColumnListItem>
                    <Text text="{ProductID}"/>
                    <Text text="{ProductName}"/>
                    <Text text="{UnitPrice}"/>
                </ColumnListItem>
            </items>
        </Table>
        
    </Page>
</mvc:View>